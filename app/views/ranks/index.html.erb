<h3>Here is the latest ranking</h3>


	<%= link_to "Initialize the ranking", init_ranks_path %>


<% unless @theranking.empty? %>
	<% for i in @theranking do %>
		<%= i[0] %>
		<small>(<%= i[1] %>)</small>
		<br>
	<% end %>

	<br>
	-------------------------
	<br>
	<% @ranks.each do |rank| %>
		<%= rank.team.city %>
		<%= rank.level %>
		<br>
	<% end %>
<% end %>